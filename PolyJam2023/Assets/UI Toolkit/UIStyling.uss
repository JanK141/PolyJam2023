.button {
    font-size: 24px;
    -unity-font-style: bold;
    width: 180px;
    height: 50px;
    -unity-text-outline-width: 0;
    -unity-text-outline-color: rgba(0, 0, 0, 255);
    -unity-slice-left: 0;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
    -unity-slice-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    justify-content: center;
    align-items: center;
    translate: 0 0;
}
